<script src="../../public/xheditor-1.2.2/xheditor_lang/zh-cn.js"></script>
<script>

    // 鼠标点击时固定active
    $(function () {
        $("#aaa").find("li").each(function () {
            var a = $(this).find("a:first")[0];
            if ($(a).attr("href") === location.pathname) {
                $(this).addClass("active");
            } else {
                $(this).removeClass("active");
            }
        });
    })

    // carousel() 轮播属性的配置
    $('.carousel').carousel({
        interval: 2000
    })

    //xheditor初始化
    $('.xheditor').xheditor({
        tools: 'simple',
        skin: 'default',
        upImgUrl: '/backStages/baikes/add/upload',
        html5Upload: false,
        upMultiple: 1,
        height: 300
    });
    //警告框关闭
    window.setTimeout(function () {
        $('[data-dismiss="alert"]').alert('close');
    }, 2000);


</script>

